{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"tsBuildInfoFile": "dist/.tsbuildinfo",
		"strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"noImplicitReturns": false,
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
	},
	"include": ["src"]
}
